@media only screen and (min-width: 992px){
  .header__heading-logo.header__desktop-logo{
    width: 200px; 
    height: 150px; 
    left: 50%;
    margin-top: -3.5rem;
    max-width: 200px;
    position: absolute;
    transition: .5s ease;
    transform: translate(-50%);
    z-index: 1;
  }
}

/* FAQ section */
.faq-section {
  padding: 30px 0;
}

.faq-section h2 {
  text-align: center;
}

.faq-question {
  color: #444;
  cursor: pointer;
  padding: 20px;
  border: none;
  outline: none;
  transition: 0.4s;
  margin: auto;
  font-weight: bold;
  border-bottom: 1px solid;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.faq-answer {
  padding: 20px;
  background-color: #f9f9f9;
  display: none;
  overflow: hidden;
}

.faq-question.active,
.faq-question:hover {
  background-color: #F9F9F9;
}

.faq-question:after {
  content: '\02795';
  font-size: 13px;
  color: #777;
  float: right;
  margin-left: 5px;
}
.faq-question.active:after {
  content: "\2796";
}

.contact-support-form #message{
  height: auto; 
}

#re-captcha{
  display: flex; 
  flex-wrap: wrap; 
  justify-content: center;
  margin: 20px 0;
}

.col.links .active:after {
display: none;
}

.col.links .active {
background: transparent;
}

.featured-image-gallery img{
  height: auto;
}

.product-featured-video--wrapper{
  background-color: #000;
  height: 0; 
  width: 100%; 
  padding-bottom: 100%; 
  position: relative;
}

.product-featured-video--wrapper video, .product-featured-video--wrapper iframe{
  position: absolute; 
  width: 100%; 
  height: auto; 
  top: 50%; 
  left: 0; 
  transform: translateY(-50%);
}

.product-featured-video--wrapper iframe{
  height: 100%;
}


/* Style for Deck Troubleshooting Form */

.deck-troubleshooting-form__input-field{
  font-size: 14px;
  margin-bottom: 20px;
  line-height: 1.2;
}

.deck-troubleshooting-form__input-field .radio-field{
  display: flex; 
  flex-wrap: wrap; 
  column-gap: 10px;
}

.input-checkbox-container, .input-radio-container{
